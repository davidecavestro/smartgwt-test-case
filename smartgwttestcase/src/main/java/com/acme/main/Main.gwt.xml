<?xml version="1.0" encoding="UTF-8"?>
<module >
	<inherits name='com.google.gwt.user.User'/>
	
		<!-- Inherit from SmartGWT. -->
<!-- 	<inherits name="com.smartgwt.SmartGwt"/> -->
	 
	<!-- Enable the developer console -->
<!-- 	<inherits name="com.smartgwt.tools.SmartGwtTools"/> -->

	<!-- Add logging support. -->
	<inherits name='com.google.gwt.logging.Logging' />
<!-- 	<set-property name='gwt.logging.logLevel' value='FINE' /> -->
<!-- 	<set-property name='gwt.logging.popupHandler' value='DISABLED' /> -->
<!-- 	<set-property name='gwt.logging.consoleHandler' value='ENABLED' /> -->
<!-- 	<set-property name='gwt.logging.firebugHandler' value='ENABLED' /> -->
	
	<!-- Make logging output dependent upon a query string value. -->
	<define-property name="dynamicLogging1" values="TRUE,FALSE"/>
 	<property-provider name="dynamicLogging1"><![CDATA[
 		{
 			if (window.location.href.indexOf("logging") != -1) {
 				return "TRUE";
 			}
 			return "FALSE";
 		}
 	]]></property-provider>
<!-- 	<set-property name='dynamicLogging' value='FALSE'/> -->
<!-- 	<set-property name='gwt.logging.logLevel' value='FINE' /> -->
	
	<set-property name="dynamicLogging1" value="FALSE"/>
	<set-property name="gwt.logging.logLevel" value="WARNING">
		<when-property-is name="dynamicLogging1" value="FALSE"/>
	</set-property>
	<entry-point class="com.acme.main.client.Main" />
</module>
